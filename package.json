  "scripts": {
    "format": "prettier --write src/**/*.{js,jsson,md} docs/*.md webpack/*.js bin/*.js",
    "build": "nice -20 rescripts build",
    "build@local": "NODE_OPTIONS=--openssl-legacy-provider nice -20 rescripts build",
    "deploy": "bash bin/deploy.sh",
    "eci": "bin/eci.js",
    "org": "node bin/org.js",
    "pull": "node bin/widget.js --pull",
    "push": "node bin/widget.js --push",
    "widget": "node bin/widget.js",
    "fetch": "node bin/fetch.js",
    "git": "node bin/git.js",
    "i18n": "NODE_ENV=development babel -f .babelrc 'src/**/*.{js,jsx,ts,tsx}'",
    "i18n:mjml": "NODE_ENV=development babel -f .babelrc.mjml 'config/email/**/*.mjml'",
    "nope.prepublish": "babel src --out-dir lib",
    "prepublishOnly": "node src/locales/index.js && rm -rf dist/* && babel src --config-file ./package.babel.config.js --out-dir dist --copy-files",
    "set": "node bin/set.js",
    "campaign": "node bin/campaign.js",
    "pushCampaign": "node bin/campaign.js --push",
    "pullCampaign": "node bin/campaign.js --pull",
    "pullTargets": "node bin/pullTargets.js",
    "start:stg": "PROCA_ENV=staging rescripts start",
    "start": "rescripts start",
    "start@local": "NODE_OPTIONS=--openssl-legacy-provider rescripts start",
    "starts": "HTTPS=true rescripts start",
    "test": "rescripts test",
    "test:debug": "rescripts --inspect-brk test --runInBand --no-cache"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^locales/(.*)$": "<rootDir>/src/locales/en/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
