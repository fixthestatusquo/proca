  "scripts": {
    "nope.format": "prettier --write '**/*.{js,css,json,md}'",
    "format": "nice -20 dprint fmt",
    "build": "nice -20 rescripts build",
    "deploy": "bash bin/deploy.sh",
    "eci": "bin/eci.js",
    "fetch": "node bin/fetch.js",
    "i18n": "NODE_ENV=development babel -f .babelrc 'src/**/*.{js,jsx,ts,tsx}'",
    "nope.prepublish": "babel src --out-dir lib",
    "wip.deploy": "rsync -azv --no-o --no-g build/* steinem:/srv/widget/d/",
    "prepublishOnly": "node src/locales/index.js && rm -rf dist/* && babel src --config-file ./package.babel.config.js --out-dir dist --copy-files",
    "pull": "node bin/pull.js",
    "push": "node bin/push.js",
    "pushCampaign": "node bin/pushCampaign.js",
    "pullCampaign": "node bin/pullCampaign.js",
    "start": "rescripts start",
    "test": "rescripts test",
    "test:debug": "rescripts --inspect-brk test --runInBand --no-cache"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^locales/(.*)$": "<rootDir>/src/locales/en/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
